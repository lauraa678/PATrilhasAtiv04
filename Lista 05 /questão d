using System;

namespace Exercicíos
{
	class Program
	{
		public static void Main()
		{
			string texto = "";
			
			string resultado = (string.IsNullOrEmpty(texto)) ? "String vazia" : "String não está vazia";
			Console.WriteLine(resultado);
			
			Console.Write("Pressione uma tecla qualquer . . . ");
			Console.ReadKey(true);
		}
	}
}
