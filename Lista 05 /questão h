using System;

namespace Exercicíos
{
	class Program
	{
		public static void Main()
		{
			string texto = "100";
			
			int resultado;
			int numero = int.TryParse(texto, out  resultado) ? resultado : 0;
			Console.WriteLine("Número convertido: " + numero);
			
			Console.Write("Pressione uma tecla qualquer . . . ");
			Console.ReadKey(true);
		}
	}
}
