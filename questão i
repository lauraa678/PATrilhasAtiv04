using System;

namespace Exercicíos
{
	class Program
	{
		public static void Main()
		{
			int num1 = 8;
			int num2 = 3;
			
			string divisivel = (num1 / num2 == 0) ?"Esses números são divisíveis um pelo outro." : "Esses números NÃO são divisíveis um pelo outro.";
			Console.WriteLine(divisivel);
			
			Console.Write("Pressione uma tecla qualquer . . . ");
			Console.ReadKey(true);
		}
	}
}
