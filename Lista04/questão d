using System;

namespace Exercícios
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("Você é assinante de algum serviço streaming? (sim ou não)");
			string ass = Console.ReadLine().ToLower();
			Console.WriteLine("Você já assistiu Supernatural? (sim ou não)");
			string serie = Console.ReadLine().ToLower();
			
			if (serie == "sim" && ass == "sim"){
				Console.WriteLine("Ótimo, agora você pode assistir à nova temporada!");
			}else{
				Console.WriteLine("Então você não sabe salvar pessoas e caçar coisas.");
			}
			
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
